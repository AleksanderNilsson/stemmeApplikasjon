@model List<stemmeApp.Models.VoteModel>

@{
    ViewBag.Title = "Vote";
}

<h2>Vote</h2>

@foreach (var item in Model) {
    <div class="card" style="width: 18rem;">
        <img class="card-img-top" img src="~/@item.picture"  width="100" height="100"/>
        <div class="card-body">
            <p class="card-text">@item.username @item.info</p>
        </div>
    </div>
        }

